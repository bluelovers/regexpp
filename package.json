{
  "name": "regexpp2",
  "version": "2.0.1",
  "description": "Regular expression parser for ECMAScript 2018.",
  "keywords": [
    "abstract",
    "annexB",
    "ast",
    "ecmascript",
    "es2015",
    "es2016",
    "es2017",
    "es2018",
    "expression",
    "parser",
    "regexp",
    "regular",
    "syntax",
    "tree",
    "validator"
  ],
  "homepage": "https://github.com/bluelovers/regexpp#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/regexpp/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/regexpp.git"
  },
  "license": "MIT",
  "author": "Toru Nagashima (https://github.com/mysticatea)",
  "main": "./index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "_postversion": "git push && git push --tags",
    "test": "nyc mocha --require ts-node/register \"test/*.ts\" --exclude \"**/*.d.ts\" --reporter dot --timeout 10000",
    "update:ids": "npx ts-node scripts/update-unicode-ids.ts",
    "update:test": "npx ts-node scripts/update-fixtures.ts",
    "watch": "_mocha \"test/*.ts\" --exclude \"**/*.d.ts\" --require ts-node/register --reporter dot --timeout 10000 --watch-extensions .ts --watch --growl",
    "npm-publish": "npm publish",
    "prepublishOnly": "echo prepublishOnly"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.0",
    "mocha": "^6.1.4"
  },
  "peerDependencies": {

  },
  "engines": {
    "node": ">=6.5.0"
  }
}
